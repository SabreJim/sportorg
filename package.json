{
  "name": "sportorg",
  "version": "0.0.1",
  "main": "server.js",
  "dependencies": {
    "@types/node": "^12.7.1",
    "aws-sdk": "latest",
    "body-parser": "^1.19.0",
    "camelcase": "^5.3.1",
    "decamelize": "^3.2.0",
    "ejs": "latest",
    "express": "latest",
    "koa-static": "^4.0.2",
    "node-uuid": "^1.4.8",
    "path": "^0.12.7",
    "pg": "^7.12.1",
    "ramda": "^0.26.1",
    "uuid": "^3.3.2",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {},
  "scripts": {
    "prestart": "npm run install-client",
    "postinstall": "npm run build",
    "start-local": "node http-server.js env=local",
    "start": "node http-server.js env=prod",
    "install-client": "cd ./sportorg-client/ && npm install && cd ..",
    "build": "cd ./sportorg-client && ng build && cd ..",
    "start-dev": "cd ./sportorg-client/ && ng serve --baseHref='/app/'"
  }
}
